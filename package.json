{
  "name": "embed",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "components/hello-world/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn test -- --watch",
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "serve": "npm run build && preact serve",
    "dev": "preact watch --template src/index.ejs",
    "dist": "microbundle --entry src/components/hello-world/index.js",
    "deploy": "NODE_ENV=production preact build  --no-prerender --clean --template src/index.ejs --service-worker false",
    "deploy-beta": "NODE_ENV=production preact build  --no-prerender --clean --template src/index.ejs --service-worker false -c preact-beta.config.js",
    "deploy-hotfix": "NODE_ENV=production preact build  --no-prerender --clean --template src/index.ejs --service-worker false -c preact-hotfix.config.js",
    "deploy-static": "NODE_ENV=production preact build  --no-prerender --clean --template src/index.ejs --service-worker false -c preact-static.config.js"
  },
  "keywords": [],
  "author": "Zouhir <zouhir@zouhir.org>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "autoprefixer": "^9.5.0",
    "dotenv": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.0",
    "jest": "^23.6.0",
    "jest-localstorage-mock": "^2.4.0",
    "jsdom": "^13.1.0",
    "jsdom-global": "^3.0.2",
    "microbundle": "^0.6.0",
    "node-sass": "^4.11.0",
    "preact-cli": "^2.2.1",
    "preact-cli-svg-loader": "^1.0.0",
    "preact-render-spy": "^1.3.0",
    "preact-render-to-string": "^3.7.0",
    "preact-widget-scripts": "^0.6.0",
    "prettier": "^1.16.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "tslint-react-a11y": "^1.0.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/classnames": "^2.2.7",
    "classnames": "^2.2.6",
    "es6-promise-promise": "^1.0.0",
    "preact": "^8.4.2",
    "preact-compat": "^3.18.4",
    "react-inlinesvg": "^0.8.4",
    "ts-loader": "^3.5",
    "webpack-s3-uploader": "^1.3.1"
  }
}
