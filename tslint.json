{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-react-a11y"
  ],
  "jsRules": {},
  "rules": {
    "member-access": [true, "no-public"],
    "jsx-no-multiline-js": false,
    "quotemark": [true, "double"],
    "semicolon": [
      true,
     "always",
     "ignore-interfaces"
    ],
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}]
  },
  "rulesDirectory": []
}
