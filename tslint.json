{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "jsRules": {},
  "quotemark": [true, "double"],
  "rules": {
    "member-access": [true, "no-public"],
    "semicolon": [
      true,
     "always",
     "ignore-interfaces"
    ],
    "trailing-comma": true
  },
  "rulesDirectory": []
}
